<%= simple_form_for(@career) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :city %>
    <%= f.input :country %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :resume %>
    <%= f.input :photo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit,{value: 'SEND',class:"btn btn-success"} %>
  </div>
<% end %>
